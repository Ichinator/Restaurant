{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent ()}}
{% endblock %}
{% block body %}
    {{ parent ()}}

    {% for entrees in entrees %}
        {% if entrees.enabled %}
            {{ entrees.name}}<br \>
            {{ entrees.prix }}<br \>
            <img src="{{ vich_uploader_asset(entrees, 'imageFile') }}" alt="{{ entrees.image }}" /><br \><br \>
        {% endif %}
    {% endfor %}

{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}