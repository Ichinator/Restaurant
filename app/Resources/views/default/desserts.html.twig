{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent ()}}
{% endblock %}
{% block body %}
    {{ parent ()}}

    {% for desserts in desserts %}
        {% if desserts.enabled %}
            {{ desserts.name}}<br \>
            {{ desserts.prix }}<br \>
            <img src="{{ vich_uploader_asset(desserts, 'imageFile') }}" alt="{{ desserts.image }}" /><br \><br \>
        {% endif %}
    {% endfor %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}